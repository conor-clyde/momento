{
  "expo": {
    "name": "Momento",
    "slug": "Momento-Project-App",
    "scheme": "momento",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8FAFC"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stringcage.momento",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Momento needs access to your camera to capture photos for your moments.",
        "NSPhotoLibraryUsageDescription": "Momento needs access to your photo library to display your captured moments.",
        "NSPhotoLibraryAddUsageDescription": "Momento needs permission to save photos to your camera roll."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#F8FAFC",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.stringcage.momentoprojectapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a9605649-f9ae-42a0-a630-dfe16b8fd0b0"
      }
    }
  }
}