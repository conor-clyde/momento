{
  "expo": {
    "name": "Momento",
    "slug": "momento",
    "icon": "./assets/images/icon.png",
    "description": "A beautiful mobile app for capturing and reflecting on meaningful moments - big or small. Take photos, add moods and notes, track daily streaks, and build mindful habits.",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "android": {
      "package": "com.conorclyde.momento",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO"
      ],
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png"
      },
      "blockedPermissions": [
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.conorclyde.momento",
      "icon": "./assets/images/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3dc58e68-bdd9-4447-a45a-7f8f4317b9aa"
      }
    },
    "owner": "stringcage"
  }
}