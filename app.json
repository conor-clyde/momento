{
  "expo": {
    "name": "Momento",
    "slug": "momento",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "android": {
      "package": "com.conorclyde.momento",
      "permissions": [
        "CAMERA"
        // Note: Media library permissions are automatically added by expo-media-library
        // RECORD_AUDIO, VIBRATE, and SYSTEM_ALERT_WINDOW removed from AndroidManifest.xml
        // as they are not needed for the app's core functionality
      ]
    },
    "ios": {
      "bundleIdentifier": "com.conorclyde.momento"
    },
    "extra": {
      "eas": {
        "projectId": "3dc58e68-bdd9-4447-a45a-7f8f4317b9aa"
      }
    }
  }
}
